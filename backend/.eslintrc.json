{
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "node"
    ],
    "rules": {
      "no-console": "warn",  
      "no-unused-vars": "warn", 
      "node/no-unsupported-features/es-syntax": [
        "error",
        {
          "version": "14",
          "ignores": []
        }
      ]
    }
  }
  